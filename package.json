{
  "name": "languageforce",
  "version": "1.0.0",
  "description": "Langueforce",
  "scripts": {
    "rimraf": "rimraf",
    "clean:dist": "npm run rimraf -- dist",
    "build:dev": "npm run clean:dist && webpack --env=dev --progress --profile --colors",
    "build:dist": "npm run clean:dist && webpack --env=prod --progress --profile --bail --colors",
    "server:dev": "webpack-dev-server --env=dev --open --progress --profile --watch --content-base src/",
    "server:prod": "http-server dist -c-1 --cors",
    "ci:travis": "npm run lint && npm run build:dist",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "server": "npm run server:dev",
    "start": "npm run server",
    "tslint": "tslint"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/common": "^2.4.10",
    "@angular/compiler": "^2.4.10",
    "@angular/core": "^2.4.10",
    "@angular/forms": "^2.4.10",
    "@angular/http": "^2.4.10",
    "@angular/platform-browser": "^2.4.10",
    "@angular/platform-browser-dynamic": "^2.4.10",
    "@angular/router": "~3.4.0",
    "core-js": "^2.4.1",
    "http-server": "^0.9.0",
    "rxjs": "^5.2.0",
    "zone.js": "^0.7.8"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.4.10",
    "@types/node": "^7.0.12",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.12",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.5",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.16.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.1",
    "node-sass": "^4.5.1",
    "null-loader": "^0.1.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "tslib": "^1.6.0",
    "tslint": "^5.1.0",
    "typescript": "^2.2.1",
    "uglify-js": "^2.8.16",
    "webpack": "^2.3.2",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^3.0.0"
  },
  "repository": {}
}
